{
  "expo": {
    "name": "workerApp",
    "slug": "workerApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "workerapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.urbanlite.worker"
,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "BASE_URL": "http://192.168.0.169:5000",
      "ROUTER_IP": "192.168.0.169",
      "PORT_BOOKINGS": "5000",
      "router": {},
      "eas": {
        "projectId": "94a0082a-9ec3-4494-a1c8-181b7a2e2b82"
      }
    },
    "owner": "ashish.teckfile"
  }
}
